<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/start_backg"
    android:orientation="vertical"
    tools:context=".StartActivity">

    <Button
        android:id="@+id/student_login"
        android:layout_width="150dp"
        android:layout_height="150dp"
        android:background="@drawable/student_logo"
        android:layout_marginTop="60dp"
        android:layout_marginLeft="20dp"
        android:onClick="LunchLoginMenu"/>

    <Button
        android:layout_width="150dp"
        android:layout_height="150dp"
        android:background="@drawable/admin_logo"
        android:layout_marginTop="60dp"
        android:layout_marginLeft="20dp"
        android:onClick="LunchSignUpMenuMenu"/>


</LinearLayout>